{   "authors": ["Manoj Plakal", "Dan Ellis"],
    "references": ["", "https://github.com/tensorflow/models/tree/master/research/audioset/yamnet"],
    "downstream_tasks": ["audio_classification"],
    "install_cmds": ["pip install torch-vggish-yamnet",
                     "pip install numpy==2.0",
                     "pip install pyyaml"],
    "import_cmds": ["from torch_vggish_yamnet import yamnet",
                    "from torch_vggish_yamnet.input_proc import WaveformToInput"],
    "model_init": ["model = yamnet.yamnet(pretrained=True)"],
    "sample_rate": 16000,
    "in_shape": 2,
    "pre_proc": ["pre_processor = WaveformToInput()",
                 "x = pre_processor(x, 16000)"],
    "embed_fwd": ["y, _ = model(x)"]
}