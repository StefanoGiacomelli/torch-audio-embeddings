                                        "in_shape": 2,
                                        "embed_fwd": ["y, timestamps = torchopenl3.get_audio_embedding(audio=x, sr=48000, model=model, center=True, hop_size=0.1, batch_size=x.shape[0], verbose=False)"]
                                    }
}