    "torchopenl3_env_linear_512":   {   "install_cmd": ["pip install setuptools",
                                                        "pip install torchopenl3", 
                                                        "pip install resampy",
                                                        "pip install numpy==2.0"],
                                        "import": ["import torchopenl3"],
                                        "model_init": ["model = torchopenl3.models.load_audio_embedding_model(input_repr='linear', content_type='env', embedding_size=512)"],
