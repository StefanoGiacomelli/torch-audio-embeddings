{   "authors": ["Xubo Liu", "Haohe Liu", "Qiuqiang Kong", "Xinhao Mei", "Mark D. Plumbley", "Wenwu Wang"],
    "references": ["https://ieeexplore.ieee.org/document/10096211", "https://github.com/liuxubo717/SimPFs"],
    "downstream_tasks": ["audio_classification"],
    "install_cmds": ["git clone https://github.com/liuxubo717/SimPFs.git",
                     "pip install torchlibrosa",
                     "pip install torchinfo"],
    "import_context": "./SimPFs",
    "import_cmds": ["import sys",
                    "import os",
                    "sys.path.insert(0, os.getcwd())",
                    "import torch",
                    "from main import Audio_Encoder",
                    "from network import Audio_Frontend, PANNS_Cnn10",
                    "from pooling import Pooling_layer"],
    "model_init_context": "./SimPFs",
    "model_init": ["panns_params = {'sample_rate': 32000, 'window_size': 1024, 'hop_size': 320, 'mel_bins': 64, 'fmin': 50, 'fmax': 14000}",
                   "sampler = Pooling_layer(factor=2.)",
                   "frontend = Audio_Frontend(**panns_params, sampler=sampler)",
                   "backbone = PANNS_Cnn10(pretrained=False)",
                   "model = Audio_Encoder(frontend=frontend, backbone=backbone)"],
    "sample_rate": 32000,
    "in_shape": 2,
    "embed_fwd": ["y = model(x)",
                  "y = y['embedding']"]
}